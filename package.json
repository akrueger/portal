{
  "name": "portal",
  "version": "0.0.1",
  "description": "Personal projects",
  "scripts": {
    "start:dev": "npm run lint && npm run clean && npm run build:dev && webpack-dev-server --inline --hot",
    "start:prod": "npm run lint && npm run clean && npm run build:prod && node server.js",
    "build:dev": "npm run lint && npm run clean && webpack",
    "build:prod": "npm run lint && npm run clean && webpack -p --config webpack.config.production.js",
    "lint": "eslint . --ext .js --ext .jsx --cache",
    "clean": "rm -rf build/*",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akrueger/portal.git"
  },
  "author": "Andrew Krueger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akrueger/portal/issues"
  },
  "homepage": "https://github.com/akrueger/portal#readme",
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "ava": "^0.15.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "poststylus": "^0.2.3",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.2",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "yeticss": "^7.3.0"
  },
  "dependencies": {
    "koa": "^1.2.1",
    "koa-cache-control": "^1.0.0",
    "koa-compress": "^1.0.9",
    "koa-static": "^2.0.0",
    "koa-sslify": "^1.0.1"
  }
}
